@{
    ViewBag.Title = "consumo";
   
}
<h2>Listado de Productos</h2>


<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Título</th>
            <th>Mensaje</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
        <tr>
            <td>@product.id</td>
            <td>@product.title</td>
            <td>@product.body</td>
        </tr>
        }
    </tbody>
</table>